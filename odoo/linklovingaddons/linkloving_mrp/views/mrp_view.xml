<openerp>    <data>        <record id='mrp_bom_tree_view' model='ir.ui.view'>            <field name='name'>mrp.bom.tree</field>            <field name='model'>mrp.bom</field>            <field name='arch' type='xml'>                <tree>                    <field name="default_code"/>                    <field name="name"/>                    <field name="create_date"/>                </tree>            </field>        </record>        <record id="mrp.product_template_action" model="ir.actions.act_window">            <field name="name">Products</field>            <field name="res_model">product.template</field>            <field name="view_type">form</field>            <field name="view_mode">tree,kanban,form</field>            <field name="context">{}</field>        </record>        <record id="product_product_action" model="ir.actions.act_window">            <field name="name">ProductProduct</field>            <field name="res_model">product.product</field>            <field name="view_type">form</field>            <field name="view_mode">tree,kanban,form</field>            <field name="context">{}</field>        </record>        <menuitem action="product_product_action" id="menu_stock_product_product_menu" parent="stock.menu_stock_product"                  sequence="10"/>        <record id="linkloving_mrp_bom_tree_view" model="ir.ui.view">            <field name="name">mrp.bom.tree</field>            <field name="model">mrp.bom.line</field>            <field name="inherit_id" ref="mrp.mrp_bom_tree_view"/>            <field name="arch" type="xml">                <xpath expr="//field[@name='product_uom']" position="before">                    <field name="product_qty"/>                    <field name="line_locate"/>                </xpath>            </field>        </record>        <record id="linkloving_mrp_bom_form_view" model="ir.ui.view">            <field name="name">mrp.bom.form</field>            <field name="model">mrp.bom</field>            <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>            <field name="arch" type="xml">                <xpath expr="//field[@name='product_id']" position="after">                    <field name="is_pcba"/>                </xpath>                <xpath expr="//field[@name='bom_line_ids']" position="replace">                    <field name="bom_line_ids" widget="one2many_list" context="{'default_is_pcba':is_pcba}">                        <tree string="Components" editable="bottom">                            <field name="sequence"/>                            <field name="product_id" on_change="onchange_product_id(product_id, product_qty)"/>                            <field name="product_sepcs"/>                            <field name="type"/>                            <field name="product_qty"/>                            <field name="product_uom" on_change="onchange_uom(product_id, product_uom)"                                   groups="product.group_uom"/>                            <field name="product_rounding"/>                            <field name="product_efficiency"/>                            <field name="is_pcba" invisible="1"/>                            <field name="line_locate" attrs="{'invisible': [('is_pcba','=',False)]}"/>                            <field name="date_start"/>                            <field name="date_stop"/>                            <field name="attribute_value_ids" widget="many2many_tags"                                   domain="[('product_ids.product_tmpl_id', '=', parent.product_tmpl_id)]"/>                            <field name="property_ids" widget="many2many_tags"/>                        </tree>                    </field>                    <!--<field name="is_pcba" invisible="1"/>-->                    <!--<field name="line_locate" attrs="{'invisible': [('is_pcba','=',False)]}"/>-->                </xpath>            </field>        </record>        <!--<record id="mrp.mrp_bom_tree_view" model="ir.ui.view">-->        <!--<field name="name">mrp.bom.tree</field>-->        <!--<field name="model">mrp.bom.line</field>-->        <!--<field name="field_parent">child_line_ids</field>-->        <!--<field name="arch" type="xml">-->        <!--<tree string="Bill of Materials">-->        <!--<field name="sequence" invisible="1"/>-->        <!--<field name="product_id"/>-->        <!--<field name="product_uom" groups="product.group_uom"/>-->        <!--<field name="type"/>-->        <!--<field name="product_qty"/>-->        <!--<field name="routing_id" groups="mrp.group_mrp_routings"/>-->        <!--<field name="date_start"/>-->        <!--<field name="date_stop"/>-->        <!--</tree>-->        <!--</field>-->        <!--</record>-->    </data></openerp>