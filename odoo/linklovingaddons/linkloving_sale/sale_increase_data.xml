<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1"> <!--【1】重装之后才会从 number_next 开始计数-->

        <record id="sequence_type_cn" model="ir.sequence.type">
            <field name="name">Sequence_cn</field>
            <field name="code">cn.partner.internal.code</field>
        </record>

        <record id="sequence_cn" model="ir.sequence">
            <field name="name">internalcodeCN</field>
            <field name="code">cn.partner.internal.code</field>
            <field name="prefix">DC</field>
            <field name="number_next">1000</field>
            <field name="padding">4</field>
        </record>

        <record id="sequence_type_en" model="ir.sequence.type">
            <field name="name">Sequence_en</field>
            <field name="code">en.partner.internal.code</field>
        </record>

        <record id="sequence_en" model="ir.sequence">
            <field name="name">internalcodeEN</field>
            <field name="code">en.partner.internal.code</field>
            <field name="prefix">OC</field>
            <field name="number_next">1000</field>
            <field name="padding">4</field>
        </record>

    </data>
</openerp>
